<block wx:if="{{bfpage}}">
  <view class="Playbackpage" style="background:{{back}}">
     <text class="bfpagereturn" bindtap="setbfpage"></text>
  </view>
</block>
<block wx:if="{{bfbtn}}">
  <view class="bfbtn" style="background:{{back}}" bindtap="setbfpage">
    <image class="gimage" src="../../../images/bofangbtn.png"></image>
    <text style="color:{{colors}};">春娇与志明</text>
  </view>
</block>
<image wx:if="{{bfbtn?bottbfimgbtn:false}}" bindtap="setbottbtn" class="bottztbtn" src="../../../images/bofangbtn.png"></image>
<image wx:elif="{{bfbtn}}" bindtap="setbottbtn" class="bottztbtn" src="../../../images/bofangzhongbtn.png"></image>

<cover-view class="tabBarbox" style="background:{{back}};" wx:if="{{tabbarkg}}">
  <cover-view class="intabBarbox" bindtap="switchTab" wx:for="{{list}}" wx:key="index" data-index="{{index}}" data-path="{{item.pagePath}}">
    <cover-image class="intabBarimg" src="{{selected === index ? item.selectedIconPath : item.iconPath}}"></cover-image>
    <cover-view class="intabBartext" style="color:{{selected == index ? colors : boxcolor}};">{{item.text}}</cover-view>
  </cover-view>
</cover-view>