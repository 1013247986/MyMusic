.home{
  height: 100%;
}
.homecenterbox{
  width: 90%;
  margin: 25rpx auto;
  height: 400rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-content: space-between;
}
.inhomecenterbox{
  height: 46.5%;
  width: 48%;
  background: rgb(111, 111, 236);
  border: 1px solid transparent;
  border-radius: 10rpx;
}

.homeRecommend{
  padding: 0 20rpx;
  height: 42rpx;
  font-size:28rpx;
  margin-bottom: 30rpx;
}
.homeRecommend .left{
  font-weight: 900;
}
.homeRecommend .right{
  float: right;
  font-size: 24rpx;
  width: 120rpx;
}
.homeRecommend .rightbtn{
  display: inline-block;
  height: 14rpx;
  width: 14rpx;
  transform: rotateZ(45deg);
  border-top: 4rpx solid #F9FAF5;
  border-right: 4rpx solid #F9FAF5;
  margin:10rpx 0 0 20rpx;
}