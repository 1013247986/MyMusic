.home{
  height: 100%;
}
.homecenterbox{
  width: 90%;
  margin: 25rpx auto;
  height: 400rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-content: space-between;
}
.inhomecenterbox{
  height: 46.5%;
  width: 48%;
  border: 1px solid transparent;
  border-radius: 10rpx;
  position: relative;
}
.inhomecenterbox image{
  width: 100%;
  height: 100%;
  border: 0.1rpx solid transparent;
  border-radius: 10rpx;
  box-sizing: border-box;
}
.inhomecenterbox text{
  position: absolute;
  top: 50%;
  left:50%;
  transform: translate(-50%,-50%);
  letter-spacing: 6rpx;
  font-weight: 900;
  text-shadow: 2rpx 2rpx 10rpx #12B7F5;
}
.lunboimg{
  width: 94%;
  height: 100%;
  margin-left: 3%;
  border: 1px solid transparent;
  box-sizing: border-box;
  border-radius:10rpx; 
}
.homeRecommend{
  padding: 0 20rpx;
  height: 42rpx;
  font-size:28rpx;
  margin-bottom: 20rpx;
}

.homeRecommend .left{
  font-weight: 900;
}
.homeRecommend .right{
  float: right;
  font-size: 24rpx;
  width: 120rpx;
}
.homeRecommend .rightbtn{
  display: inline-block;
  height: 14rpx;
  width: 14rpx;
  transform: rotateZ(45deg);
  border-top: 4rpx solid #F9FAF5;
  border-right: 4rpx solid #F9FAF5;
  margin:10rpx 0 0 20rpx;
}
.homeRecommendInBox{
  padding: 10rpx 20rpx;
  position: relative;
}
.boxinimg{
  width: 110rpx;
  height: 110rpx;
  background: red;
  border: 1px solid #F9FAF5;
  border-radius: 12rpx;
  margin-right: 26rpx;
}
.boxintoptext{
  display: inline-block;
  vertical-align: top;
  margin-top: 20rpx;
  font-size: 30rpx;
  letter-spacing: 2rpx;
}
.boxintopname{
  display: inline-block;
  color: rgb(129, 125, 125);
  font-size: 26rpx;
  position: relative;
  bottom: 20rpx;
  right: 170rpx;
}
.boxintoprightbtn{
   position: absolute;
   right: 30rpx;
   top: 40rpx;
   height: 60rpx;
   width: 60rpx;
   border-radius: 50%;
}
.boxintoprightbtn:hover{
   box-shadow: 0 0 10rpx #F9FAF5;
}